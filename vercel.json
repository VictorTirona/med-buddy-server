{
    "version": 2,
    "builds": [
        {
            "src": "server.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "rewrites": [
        {
            "source": "/api/v1/:path*",
            "destination": "https://med-buddy-server.vercel.app/api/v1/:path*"
        }
    ]
}